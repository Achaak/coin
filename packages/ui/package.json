{
  "name": "@my-coin/ui",
  "version": "0.0.0",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "format": "prettier --cache --write \"**/*.{ts,tsx,.md}\"",
    "prelint": "pnpm build",
    "lint": "cross-env TIMING=1 eslint src --fix"
  },
  "dependencies": {
    "@pikas-ui/alert": "^4.0.2",
    "@pikas-ui/avatar": "^3.0.2",
    "@pikas-ui/badge": "^2.0.2",
    "@pikas-ui/button": "^4.0.2",
    "@pikas-ui/checkbox": "^4.0.2",
    "@pikas-ui/dialog": "^3.0.4",
    "@pikas-ui/dropdown-menu": "^4.1.1",
    "@pikas-ui/grid": "^2.1.0",
    "@pikas-ui/icons": "^4.0.2",
    "@pikas-ui/loader": "^4.0.2",
    "@pikas-ui/progress": "^2.0.2",
    "@pikas-ui/searchbar": "^4.0.4",
    "@pikas-ui/select": "^5.1.4",
    "@pikas-ui/separator": "^4.0.2",
    "@pikas-ui/skeleton": "^3.0.2",
    "@pikas-ui/styles": "^4.0.2",
    "@pikas-ui/switch": "^3.0.2",
    "@pikas-ui/table": "^4.1.9",
    "@pikas-ui/tabs": "^3.0.2",
    "@pikas-ui/text": "^5.0.2",
    "@pikas-ui/textarea": "^4.0.2",
    "@pikas-ui/textfield": "^4.0.4",
    "@pikas-ui/title": "^2.0.2",
    "@pikas-ui/tooltip": "^4.0.2",
    "@stitches/react": "^1.2.8",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@my-coin/prettier": "workspace:*",
    "@my-coin/tsconfig": "workspace:*",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
