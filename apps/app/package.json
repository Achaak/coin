    "@pikas-utils/screen": "^2.1.0",
    "@radix-ui/react-accordion": "^1.1.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^4.24.10",
    "@trpc/client": "10.13.2",
    "@trpc/next": "10.13.2",
    "@trpc/react-query": "10.13.2",
    "@trpc/server": "10.13.2",
    "formik": "^2.2.9",
